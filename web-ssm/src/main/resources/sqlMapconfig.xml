    <typeAliases>
        <typeAlias type="com.qinbo.entity.Employee" alias="Employee"/>
        <typeAlias type="com.qinbo.entity.Role" alias="Role"/>
    </typeAliases>
    <mappers>
        <mapper resource="./sqlmapping/employee_sqlmap.xml"/>
        <mapper resource="./sqlmapping/role_sqlmap.xml"/>
    </mappers>
</configuration>
